<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.whalecar.persistence.UserCarMapper">
    
    <!-- 根据id查询shop -->
    <insert id="addUserCar" parameterType="Map" >
        insert into
        User_Car(
            userName,
            phoneNum,
            carID,
            carName,
            createDate
        )values(
            #{userName},
            #{phoneNum},
            #{carID},
            #{carName},
            #{createDate}
        )
    </insert>


</mapper>