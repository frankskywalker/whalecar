	<script>
		// ‘Ìá‘½»‘É¸‘Ñ¡‘ÊÂ‘¼þ
		function conditionSelect(type, value, needSubmit, needInitPage) {
			$("#form_" + type).val(value);

			if (needInitPage) {
				$("#form_pageIndex").val('');
			}

			if (needSubmit == true) {
				$("#conditionform").submit();
			}
		}

		// ‘É¸‘Ñ¡‘Ìõ‘¼þ‘Ñ¡‘Ôñ‘Ð§‘¹û
		var currentHoverBtn = null;
		var tabNames = [ "city", "brand", "model", "price" ];
		function initEventForTabs() {
			for (i in tabNames) {
				var name = tabNames[i];
				$("#" + name).hover(function() {
					currentHoverBtn = $(this).attr("id");
				}, function() {
					currentHoverBtn = null;
				});
			}
		}
		initEventForTabs();
		$(document).click(function() {
			// ‘Îª‘ÁË‘Ð§‘¹û‘£¬‘Èç‘¹û‘Ðè‘Òª‘ÏÔ‘Ê¾‘²Ë‘µ¥‘¾Í‘ÓÃdiv‘³Å‘×¡‘¸ß‘¶È‘£¬‘±Ü‘Ãâ‘Ò³‘Ãæ‘ÂÒ‘¶¯
			if (currentHoverBtn != null) {
				// $("#divHolder").css({height:"30px",display:""});
				$("#divHolder").slideDown();
			} else {
				$("#divHolder").slideUp('slow');
			}

			// ‘ÏÈ‘Òþ‘²Ø
			for (i in tabNames) {
				var name = tabNames[i];
				if (currentHoverBtn != name) {
					$("#" + name).removeClass("active");
					$("#" + name + "Div").hide();
				}
			}

			// ‘ÔÙ‘ÏÔ‘Ê¾
			for (i in tabNames) {
				var name = tabNames[i];
				if (currentHoverBtn == name) {
					$("#" + name).addClass("active");
					$("#" + name + "Div").show();
				}
			}
		});

		// ‘Æû‘³µ‘Í¼‘Æ¬‘¸¡‘¶¯‘Ð§‘¹û
		$("div[name='hoverdiv']").each(function(index, value) {
			$(value).hover(function() {
				$(this).find("div[name='imgdiv']").fadeTo('slow', 0.03);
				$(this).find("div[name='coverdiv']").fadeToggle('slow');
			}, function() {
				$(this).find("div[name='imgdiv']").fadeTo('slow', 1);
				$(this).find("div[name='coverdiv']").fadeToggle('slow');
			});
		});

		$(function() {
			$('#carousel').carouFredSel({
				width : '100%',
				items : {
					visible : 3,
					start : -1
				},
				scroll : {
					items : 1,
					duration : 1000,
					timeoutDuration : 3000
				},
				prev : '#prev',
				next : '#next',
				pagination : {
					container : '#pager',
					deviation : 1
				}
			});
		});
	</script>
